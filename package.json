  },
  "devDependencies": {
    "steal": "^0.10.0",
    "steal-tools": "^0.10.0",
    "bower": "1.3.8",
    "grunt": "~0.4.1",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-watch": "~0.3.0"
