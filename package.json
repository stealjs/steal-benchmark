  "dependencies": {
    "benchmark": "^2.1.2"
  },
  "devDependencies": {
    "steal": "^0.16.43",
    "steal-tools": "^0.16.8",
    "bower": "1.8.0",
    "grunt": "~1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-copy": "~1.0.0",
    "grunt-contrib-watch": "~1.0.0"
  }
